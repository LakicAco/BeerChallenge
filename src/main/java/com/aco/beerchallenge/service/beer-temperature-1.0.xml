<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
<!--    <preConditions>-->
<!--        <runningAs  username="liquibase"/>-->
<!--    </preConditions>-->

    <changeSet  id="Beer temperature change log 1.0"  author="alla0911">

        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="BEER_TEMPERATURES"/>
            </not>
        </preConditions>

        <createTable tableName="${prefix}ACLS" remarks="Funding ACL table">
        <column name="ID" type="BIGINT" autoIncrement="true" remarks="Primary key ID">
            <constraints nullable="false" primaryKey="true" primaryKeyName="PK_${prefix}ACLS"/>
        </column>
        <column name="TENANT_ID" type="VARCHAR(50)" remarks="Tenant ID">
            <constraints nullable="false"/>
        </column>
        <column name="FUNDING_ID" type="BIGINT" remarks="Tenant ID">
            <constraints nullable="false" foreignKeyName="FK_${prefix}ACLS_${prefix}FUNDINGS"/>
        </column>

        <!--        <createSequence sequenceName="BEER_EXECUTION_SEQ" startValue="1" incrementBy="1"/>-->
    </changeSet>


</databaseChangeLog>